{
  "name": "ecrf-analyzer",
  "version": "19.0.0",
  "scripts": {
    "ng": "ng",
    "start:server": "node server.ts",
    "start:client": "ng serve --proxy-config proxy.conf.json",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "test": "ng test",
      "server": "tsc && node dist/server.js",
      "heroku-postbuild": "ng build --prod && tsc"
  },
  "main": "server.ts",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ngrx/signals": "^19.0.1",
    "@primeng/themes": "^19.0.5",
    "axios": "^1.7.9",
    "chart.js": "4.4.2",
    "primeclt": "^0.1.5",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.5",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.3.2",
    "timers": "^0.1.1",
    "tslib": "^2.3.0",
    "xml2js": "^0.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2"
  }
}
